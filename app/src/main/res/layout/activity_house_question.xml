<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    tools:context=".HouseQuestionActivity">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="379dp"
        android:layout_height="637dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/specification_house" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/facilities_title" />

    <EditText
        android:id="@+id/area_lot"
        android:imeOptions="actionDone"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:hint="sqft"
        android:inputType="numberDecimal"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.473" />

    <EditText
        android:id="@+id/floors"
        android:imeOptions="actionDone"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:hint="level"
        android:inputType="numberDecimal"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.569" />

    <EditText
        android:id="@+id/year_built"
        android:imeOptions="actionDone"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:hint="2020"
        android:inputType="numberDecimal"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.676" />

    <EditText
        android:id="@+id/area_living"
        android:imeOptions="actionDone"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:hint="sqft"
        android:inputType="numberDecimal"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.37" />

    <EditText
        android:id="@+id/bathrooms"
        android:imeOptions="actionDone"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:hint="room"
        android:inputType="numberDecimal"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.272" />

    <EditText
        android:id="@+id/bedrooms"
        android:imeOptions="actionDone"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:hint="room"
        android:inputType="numberDecimal"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.174" />

    <Button
        android:id="@+id/button_predict"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/rounded_button"
        android:fontFamily="@font/karlabold"
        android:text="PREDICT"
        android:textColor="@color/colorWhite"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView11" />

    <Switch
        android:id="@+id/waterfront"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="400dp"
        android:textOn="1"
        android:textOff="0"
        android:shadowColor="@color/colorMain"
        android:textColorHighlight="@color/colorMain"
        android:textColorLink="@color/colorMain"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

</androidx.constraintlayout.widget.ConstraintLayout>