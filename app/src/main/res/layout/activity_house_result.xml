<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    tools:context=".HouseResultActivity">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="379dp"
        android:layout_height="637dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/specification_house" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/facilities_title" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/result_button" />



    <TextView
        android:id="@+id/area_lot"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:text="sqft"
        android:textSize="24dp"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.473" />

    <TextView
        android:id="@+id/floors"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:text="level"
        android:textSize="24dp"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.569" />

    <TextView
        android:id="@+id/year_built"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:text="2020"
        android:textSize="24dp"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.676" />

    <TextView
        android:id="@+id/area_living"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:textSize="24dp"
        android:text="sqft"
        android:fontFamily="@font/karlareguler"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.37" />

    <TextView
        android:id="@+id/bathrooms"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:text="room"
        android:textSize="24dp"
        android:fontFamily="@font/karlareguler"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.272" />

    <TextView
        android:id="@+id/bedrooms"
        android:imeOptions="actionDone"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:text="room"
        android:textSize="24dp"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.174" />

    <TextView
        android:id="@+id/waterfront"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/karlareguler"
        android:text="Yes"
        android:textAlignment="textEnd"
        android:textColorLink="@color/colorMain"
        android:textCursorDrawable="@color/colorMain"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.774" />

    <TextView
        android:id="@+id/price"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:fontFamily="@font/karlabold"
        android:text="TextView"
        android:textAlignment="textStart"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.189"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <Button
        android:id="@+id/buttonFinish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.823"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.484"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/rectangle_button" />

</androidx.constraintlayout.widget.ConstraintLayout>