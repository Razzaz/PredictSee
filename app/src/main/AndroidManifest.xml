<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.garudahacks.predictsee">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CarResultActivity"></activity>
        <activity android:name=".HouseResultActivity" />
        <activity android:name=".ThanksActivity" />
        <activity android:name=".HouseQuestionActivity" />
        <activity android:name=".CarQuestionActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".LastIntroActivity" />
        <activity android:name=".NextIntroActivity" />
        <activity android:name=".IntroActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>